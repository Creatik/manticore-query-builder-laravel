{
    "name": "avadim/manticore-query-builder-laravel",
    "description": "An easy way to use the official ManticoreSearch client in your Laravel applications",
    "keywords": [
        "laravel",
        "manticoresoftware",
        "manticoresearch",
        "manticore",
        "database",
        "query",
        "builder",
        "sql",
        "spinx",
        "spinxql",
        "search"
    ],
    "type": "library",
    "homepage": "https://github.com/aVadim483/manticore-query-builder-laravel",
    "require": {
        "php": "^7.4|^8.1",
        "ext-json": "*",
        "psr/log": ">=1.1",
        "avadim/manticore-query-builder-php": "^1.10",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0"
    },
    "suggest": {
        "ext-curl": "*",
        "monolog/monolog":"*"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Rock",
            "email": "paul@drteam.rocks"
        }
    ],
    "autoload": {
        "psr-4": {
            "avadim\\Manticore\\Laravel\\": "./src/Manticore/Laravel"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "avadim\\Manticore\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "ManticoreDb": "avadim\\Manticore\\Laravel\\Facade"
            }
        }
    }
}
